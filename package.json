    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "express-async-handler": "^1.2.0",
    "express-session": "^1.17.3",
    "express-validator": "^7.0.1",
    "http-errors": "~1.6.3",
    "luxon": "^3.4.4",
    "mongoose": "^8.1.0",
    "morgan": "~1.9.1",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "express": "^4.18.2",
    "nodemon": "^3.0.3"
  }
}
